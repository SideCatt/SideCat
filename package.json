{
  "name": "sidecat",
  "version": "0.0.1",
  "description": "React generic sidebar-category library",
  "main": "es/sidecat.js",
  "module": "es/sidecat.js",
  "repository": "git@github.com:SideCatt/SideCat.git",
  "authors": [
    "Jonathan Bruce <jbruce232@gmail.com>",
    "Sam Chun<sanghun89@gmail.com>"
  ],
  "license": "MIT",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "jest": "jest",
    "lint": "./node_modules/.bin/eslint --plugin react --max-warnings 0 src/js/**/*.{js,jsx} __tests__/**/*.{js,jsx}",
    "lint-fix": "yarn run lint --fix",
    "prepublishOnly": "rollup -c",
    "test": "yarn run lint && jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^js(.*)$": "<rootDir>/src/js$1",
      "^sass(.*)$": "<rootDir>/src/sass$1",
      "\\.(css|scss)$": "identify-obj-proxy"
    },
    "setupFiles": [
      "raf/polyfill",
      "./__tests__/support/jestsetup.js"
    ],
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "fbjs",
      "enzyme",
      "cheerio",
      "htmlparser2",
      "underscore",
      "lodash",
      "domhandler",
      "object.assign",
      "define-properties",
      "function-bind",
      "object-keys"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.11.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "node-sass": "^4.7.1",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.1.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup": "^0.52.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglify-es": "^3.2.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "ramda": "^0.25.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1"
  }
}
